#friends table tr td#friend_thumbnail {
	float:none;
	margin:0em;
	padding:0px;
	vertical-align: middle;
}

#friends table tr td#friend_info {
	width:700%;
	vertical-align: middle;
}

#friends table tr td#friends_options{
	text-align:center;
	vertical-align: middle;
}

#friends #webmail-clients {
	list-style-image:none;
	list-style-position:outside;
	list-style-type:none;
	margin:15px 0pt 15px 20px;
	overflow:auto;
	padding:0pt;
	width:500px;
}

#friends #webmail-clients li {
	float:left;
	overflow:hidden;
	padding:0pt;
	width:140px;
}


#friends #webmail-clients li input {
	float:left;
	margin-left:10px;
}

#friends #all_contacts_panel {
	float:left;
	width:485px;
}

#friends #all_contacts_panel #import_contacts {
background:transparent url(/images/icons/add.png) no-repeat scroll left center;
float:right;
font-size:95%;
margin:-16px 0pt 10pt;
padding:0pt 0pt 0pt 15px;
}

#friends #invitation_box {
	background:transparent url(/images/icons/pic_arrow_right.gif) no-repeat scroll left top;
	float:right;
	padding-left:40px;
	text-align:left;
	width:340px;
}

#friends #address_book {
	border:1px solid #666666;
	margin-left:27px;
}

#friends #address_book div.info {
	background-color:#EEEEEE;
	clear:both;
	color:#666666;
	min-width:432px;
	overflow:hidden;
	padding:9px 10px 7px;
	height:21px;
}

#friends #address_book p#contacts_count {
	text-align:right;
}

#friends #address_book p#select_all {
	text-indent:28px;
}

#friends #address_book div.info p {
	float:left;
	font-size:85%;
	margin:0pt;
	padding:0pt;
	width:49.9%;
}

#friends #address_book ol {
	margin:0pt;
	padding:0pt;
}

#friends #address_book li {
	color:#666666;
	list-style-type:none;
}

#friends #address_book ul {
	border-top:2px solid #999999;
	margin:-12px 0pt 0pt 24px;
	padding:7px 0pt 0pt;
}

#friends #contacts_toc {
	float:right;
	margin-right:-29px;
	position:relative;
	width:28px;
}

#friends #contacts_toc ol{
	padding-bottom:0.5em;
}

#friends #contacts_toc ol li {
	font-size:95%;
	margin-left:1px;
	height:19px;
}

#friends #contacts_list ul li:first-child {
	border:none;
}

#friends #contacts_list ol {
	height:550px;
	overflow:auto;
	padding:10px 20px 10px 10px;
}

#friends #contacts_list .vcard {
	border-top:1px solid #CCCCCC;
	margin:0pt;
	overflow:hidden;
	padding:7px 0pt;
	height:29px;
}

#friends #contacts_list .vcard .title {
	color:#000000;
	float:left;
	height:auto;
	margin:0pt;
	padding:0pt;
	width:55%;
}

#contacts_list .vcard img {
	float:right;
	margin-right:4px;
}

#friends #contacts_list .vcard .contact {
	float:left;
	margin:0pt 15px 0pt 0pt;
	overflow:hidden;
	width:80%;
}

#friends #all_contacts_panel .chk {
	float:left;
	margin-right:4px;
	text-align:right;
	width:20px;
}

#friends #contacts_toc a {
	font-weight:bold;
}

#friends #contacts_toc a, #contacts_toc span {
	display:block;
	padding:0px 10px 0px 9px;
	margin:0px;
	text-align:center;
}

#friends #all_contacts_panel a {
	text-decoration:none;
}

#friends .selected_contacts {
	border:1px solid #666666;
	background:#FFFFFF none repeat scroll 0%;
	height:250px;
	overflow-x:hidden;
	overflow-y:auto;
	width:338px;
}

#friends .selected_contacts ul {
	list-style-image:none;
	list-style-position:outside;
	list-style-type:none;
	margin:5px;
	padding:0pt;
}

#friends .selected_contacts ul li {
	background:transparent url(/images/icons/cancel.png) no-repeat scroll 0pt;
	color:#666666;
	margin-bottom:2px;
	padding-left:17px;
	text-decoration:none;
}

#friends #selected_contacts_info {
	background:transparent none repeat scroll 0%;
	padding:0pt;
}

#friends .selected_contacts li p {
	background:#FFF8DB none repeat scroll 0%;
	color:#666666;
	display:inline;
}

#friends #all_contacts_panel .hilite {
	background:#FFF8DB none repeat scroll 0%;
}

#friends #all_contacts_panel #deleteSelectedContacts {
	margin:10px 0pt 0pt 61px;
	padding:2px 8px 1px;
	vertical-align:middle;
}

#friends #address_book ol li.result-message p {
background-color:#FFF8DB;
border-color:#CCCCCC;
border-style:solid;
border-width:1px 0pt;
color:#000000;
height:auto;
margin:12px 0pt 0pt 12px;
text-align:center;
}

#friends #address_book ol li.result-message p a {
display:block;
font-size:180%;
text-decoration:underline;
}

#friends #educations, #friends #experiences{
	margin:10pt;
}

#friends p.last {
	color:#828282;
	float:right;
	margin-top:-15px;
}

#friends .entry {
	margin:0pt;
	padding-left:30px;
	padding-bottom:50px;
	width:555px;
}

#friends #invite_textarea_div{
	padding-bottom:10px;
}